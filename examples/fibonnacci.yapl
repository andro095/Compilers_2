class Main {
    io_enter: IO;
    num: Int;
    count: Int <- 0;
    lat: Int <- 0;
    ant: Int <- 1;
    output: Int;
    
    fibonnacci(n: Int) : Int {
       {
        while count < n loop {
            output <- lat + ant;
            lat <- ant;
            ant <- output;
            count <- count + 1;
        } pool;
        output;
       }
    } ;


    main() : Int {
        {
            num <- (io_enter.in_int());
            fibonnacci(num);    
            
        }
    };
};